                            <option value="58">58</option>
                            <option value="59">59</option>

                            <!-- options for minutes here -->
                        </select>
                    </div>
                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1">Est. to complete loading cargo  </label>
                    <input type="text" class="form-control input-sm" id="est_to_complete_loading_cargo" />
                </div>

            </div>
            <div class="row">

                <div class="col-md-3">
                    <label for="exampleInputEmail1">Plan for sailing   </label>
                    <input type="text" class="form-control input-sm" id="plan_of_sailing" />
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <div class="form-group">
                            <label for="exampleInputEmail1">How Many days vessel will stay at load port</label>
                            <select id="vessels_stay" class="form-control">
                                <option value="0">--Select--</option>
                                <option value="1">1 Day</option>
                                <option value="2">2 Day</option>
                                <option value="3">3 Day</option>
                                <option value="4">4 to 60 Days.</option>
                            </select>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="box-header with-border" id="inward_section">
            <h3 class="box-title">BL Related Information</h3>
            <label style="float:right">
            </label>
        </div>
        <div class="box-body" id="inward">
            <div class="row">
                <div class="col-md-3">
                    <label for="exampleInputEmail1">BL NO.<span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control" id="blno" name="blno" />
                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1">SHIPPER(SH)<span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("shipper_name", new SelectList(Model.vm_shipper, "id", "shipper_name"), new { @class = "form-control input-sm", @id = "shipper_name" })
                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1"> CONSIGNEE(CN)<span class="mandatoryIndicator">*</span> </label>
                    <input type="text" class="form-control" id="consignee" name="consignee" />
                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1"> NOTIFY(NY) <span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("ReceiverId", new SelectList(Model.masterreciver, "id", "receiver_name"), new { @class = "form-control input-sm", @id = "notify" })
                </div>

            </div>
            <div class="row">
                <div class="col-md-3">
                    <label for="exampleInputEmail1">
                        MARKS & NOS
                        UNIT NO<span class="mandatoryIndicator">*</span>
                    </label>
                    @Html.DropDownList("marks_and_nos_name", new SelectList(Model.vm_marks_and_nos, "id", "marks_and_nos_name"), new { @class = "form-control input-sm", @id = "marks_and_nos_name" })
                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1">TYPE OF CARGO<span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("cargo_type_name", new SelectList(Model.Master_cargo_type, "id", "cargo_type_name"), new { @class = "form-control input-sm", @id = "cargo_type_name" })
                </div>

                <div class="col-md-3">
                    <label for="exampleInputEmail1">QUANTITY & KIND OF CARGO<span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("Quantity_and_Kind_of_cargo_name", new SelectList(Model.quantity_and_Kind_of_cargo, "id", "Quantity_and_Kind_of_cargo_name"), new { @class = "form-control input-sm", @id = "quantity_and_kind_of_cargo_name" })

                </div>
                <div class="col-md-3">
                    <label for="exampleInputEmail1">GROSS WEIGHT OF CARGO (MT)<span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control" id="quantity" name="quantity" />
                </div>
            </div>

            <div class="row">

                <div class="col-md-3">
                    <label for="exampleInputEmail1">UNIT OF CARGO<span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("Master_quantity_cargo", new SelectList(Model.Master_quantity_cargo, "id", "qtt_name"), new { @class = "form-control input-sm", @id = "qtt_name" })
                </div>
                <div class="col-md-3" style="margin-top:27px;">
                    <input type="checkbox" class="minimal" checked id="is_cleanonBoard">
                    <label for="exampleInputEmail1"> &nbsp; SETUP : CLEAN ON BOARD</label>
                </div>

            </div>
            <div class="col-md-12" style="margin-top:23px">
                <div style="float:right">
                    <a href="#" class="btn btn-sm btn-success mybtn" id="mybtn"> Add</a>
                </div>
            </div>
            <div class="col-md-3" style="display:none;">
                <label for="exampleInputEmail1">
                    Type of Cargo(DESCRIPTION OF GOODS)
                </label>
                <input type="text" class="form-control" id="cargotype_desc" name="cargotype_desc" />
            </div>
        </div>

        <div class="box-body" id="manifest">
            <div class="table-responsive">
                <table class="table table-responsive table-hover table-striped" id="tbl_InBoundManiFest">
                    <thead id="theadIssuesResult" class="customThead">
                        <tr>
                            @*<th>Action</th>*@
                            <th>BL No</th>
                            <th>Shipper</th>
                            <th>Consignee</th>
                            <th>Notify</th>
                            <th>Marks and Nos</th>
                            <th>
                                Gross Weight of Cargo
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <input type="hidden" id="CanView" value="@ViewBag.CanView" />
    <input type="hidden" id="IsDownload" value="@ViewBag.IsDownload" />
    <input type="hidden" id="CanUpdate" value="@Session["CanUpdate"]" />
    <input type="hidden" id="CanDelete" value="@Session["CanDelete"]" />
    <input type="hidden" id="CanPrint" value="@ViewBag.CanPrint" />
    <input type="hidden" id="CanVerify" value="@Session["CanVerify"]" />
