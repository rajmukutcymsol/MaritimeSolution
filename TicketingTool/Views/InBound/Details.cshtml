        .section {
            margin-top: 20px;
        }

        .row {
            display: flex;
            justify-content: space-between;
            margin-top: 10px;
        }

        .column {
            flex: 1;
        }

        .label {
            font-weight: bold;
        }

        .contact-info {
            margin-top: 20px;
        }

            .contact-info p {
                margin: 5px 0;
            }
    </style>
</head>
