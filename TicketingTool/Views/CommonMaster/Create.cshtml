@model TicketingTool.Models.ViewModel.vm_master
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/dist/css/custom.css" rel="stylesheet" />
<section class="content">
    <input type="text" style="display:none" id="mainid" />
    <div class="box box-primary box-solid bg-info">
        <div class="box-header with-border">
            <h3 class="box-title">Create Common Master</h3>
            <div class="box-tools pull-right">
                <a href="#" class="btn btn-sm btn-success btnsave" id="btnSave"> Save</a>

                <a href="/CommonMaster/Index" class="btn btn-sm btn-danger btnCancel" id="btnCancel"> Cancel</a>
            </div>
        </div>
        <div class="box-body">
            <div class="row">
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Master Type <span class="mandatoryIndicator">*</span></label>
                    @Html.DropDownList("Title", new SelectList(Model.mastertype, "id", "category_name"), new { @class = "form-control input-sm", @id = "category_name" })
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Company Name <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="CompanyName" placeholder="Company Name">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Person Incharge <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="personincharge" placeholder="Person Incharge">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Full Address</label>
                    <input type="text" class="form-control input-sm" id="Address" placeholder="Full Address Name">
                </div>

                <div class="form-group col-md-3">
                    <label for="exampleInputEmail1">Country </label>
                    @Html.DropDownList("CountryID", new SelectList(Model.mastercountry, "CountryID", "CountryName"), new { @class = "form-control", @id = "CountryID" })
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">State/Region <span class="mandatoryIndicator">*</span></label>
                    <select id="stateid" name="stateid" class="form-control">
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">City <span class="mandatoryIndicator">*</span></label>
                    <select id="cityid" name="cityid" class="form-control">
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Mobile Number <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="MobileNumber" placeholder="Mobile Number">
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Fax Number</label>
                    <input type="text" class="form-control input-sm" id="Fax" placeholder="Fax Number">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Email address <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="Email" placeholder="Email address">
                </div>

                <div class="form-group col-md-3">
                    <label style="margin-top: 27px">
                        <input type="checkbox" class="minimal" id="is_active">
                        &nbsp; Is Active
                    </label>
                </div>
            </div>
        </div>
        <div class="box-header with-border">
            <h3 class="box-title">Add More Person In-Charge</h3>
        </div>
        <div class="box-body" id="addpersonincharge">
            <div class="row">
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Person Incharge <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="PicName" placeholder="Person Incharge">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Designation <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="PicDesignation" placeholder="Mobile Number">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Mobile Number <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="PicPhone" placeholder="Mobile Number">
                </div>
                <div class="form-group col-md-3">
                    <label for="exampleInputtext1">Email address <span class="mandatoryIndicator">*</span></label>
                    <input type="text" class="form-control input-sm" id="PicEmail" placeholder="Email address">
                </div>
                <div class="form-group col-md-3">
                        <label style="margin-top: 27px">
                            <input type="checkbox" class="minimal" id="is_active_add">
                            &nbsp; Is Active
                        </label>
                    </div>
                <div class="col-md-9" style="margin-top:23px">
                    <div style="float:right">
                        <a href="#" class="btn btn-sm btn-success mybtn" id="mybtn"> Save</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="box-header with-border">
            <h3 class="box-title">List of Person In-Charge</h3>
        </div>
        <div class="box-body">

            <input type="hidden" id="id" />
            <div class="box box-primary box-solid">
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table table-responsive table-hover table-striped" id="tbl_Vendor">
                            <thead id="theadIssuesResult" class="customThead">
                                <tr>
                                    <th>Action</th>
                                    <th>Person Incharge</th>
                                    <th>Designation </th>
                                    <th>Mobile Number</th>
                                    <th>Email address</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>
@section scripts{
    <script src="~/Content/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/Content/custom_scripts/settings/CommomMaster_Create.js"></script>

    @*<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @Scripts.Render("~/bundles/common")
    <script src="~/Content/custom_scripts/settings/CommomMaster_Create.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.min.js"></script>
    <link href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/themes/blitzer/jquery-ui.css" el="Stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.13.0/themes/base/jquery-ui.css">*@
}

